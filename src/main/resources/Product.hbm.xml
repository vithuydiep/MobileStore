<?xml version = "1.0" encoding = "utf-8"?> <!DOCTYPE hibernate-mapping PUBLIC

"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name = "Model.Product" table = "Product">
<id name = "productId" type = "int" column = "ID_PRODUCT">

<generator class="assigned" />

</id>

<property name = "productName" column = "PRODUCT_NAME" type = "string"/>
<property name = "unitPrice" column = "UNIT_PRICE" type = "int"/>
<property name = "unitInStock" column = "UNIT_IN_STOCK" type = "int"/>
<property name = "description" column = "DESCRIPTION" type = "string"/>
<property name = "manufacture" column = "MANUFACTURER" type = "string"/>
<property name = "category" column = "CATEGORY" type = "string"/>
<property name = "condition" column = "PRODUCT_CONDITION" type = "string"/>
<property name = "productImage" column = "PRODUCT_IMAGE" type = "string"/>
<property name = "adminID" column = "ID_ADMIN" type = "int"/>

</class>

</hibernate-mapping>